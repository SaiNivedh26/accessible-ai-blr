  },
  {
    "text": "है। वैसी सात ऑर्डिनेंस फैक्ट्रियों को",
    "start": 675.959,
    "duration": 10.161
  },
  {
    "text": "हमने बड़ी आधुनिक कंपनियों में बदल दिया",
    "start": 680.44,
    "duration": 8.32
  },
  {
    "text": "है। आज यूपी",
    "start": 686.12,
    "duration": 6.279
  },
  {
    "text": "में देश का बड़ा",
    "start": 688.76,
    "duration": 6.48
  },
  {
    "text": "डिफेंस कॉरिडोर बन रहा",
    "start": 692.399,
    "duration": 5.641
  },
  {
    "text": "है। इस कॉरिडोर",
    "start": 695.24,
    "duration": 6.08
  },
  {
    "text": "का कानपुर",
    "start": 698.04,
    "duration": 8.6
  },
  {
    "text": "नोड रक्षा क्षेत्र में आत्मनिर्भर भारत का",
    "start": 701.32,
    "duration": 7.44
  },
  {
    "text": "बड़ा केंद्र",
    "start": 706.64,
    "duration": 4.52
  },
  {
    "text": "है।",
    "start": 708.76,
    "duration": 5.199
  },
  {
    "text": "साथियों एक",
    "start": 711.16,
    "duration": 7.76
  },
  {
    "text": "समय जहां से पारंपरिक उद्योग पलायन कर रहे",
    "start": 713.959,
    "duration": 6.841
  },
  {
    "text": "थे।",
    "start": 718.92,
    "duration": 4.2
  },
  {
    "text": "वहां अब",
    "start": 720.8,
    "duration": 8.479
  },
  {
    "text": "डिफेंस सेक्टर की बड़ी कंपनियां आ रही है।",
    "start": 723.12,
    "duration": 8.76
  },
  {
    "text": "यहां पास में ही अमेठी",
    "start": 729.279,
    "duration": 6.961
  },
  {
    "text": "में एक के203 राइफल का निर्माण शुरू हो",
    "start": 731.88,
    "duration": 6.16
  },
  {
    "text": "चुका",
    "start": 736.24,
    "duration": 5.719
  },
  {
    "text": "है। ऑपरेशन सिंदूर",
    "start": 738.04,
    "duration": 7.72
  },
  {
    "text": "में जिस ब्रह्मोस मिसाइल ने दुश्मनों को",
    "start": 741.959,
    "duration": 5.841
  },
  {
    "text": "सोने नहीं",
    "start": 745.76,
    "duration": 6.68
  },
  {
    "text": "दिया उस ब्रह्मोस मिसाइल का भी नया पता",
    "start": 747.8,
    "duration": 9.2
  },
  {
    "text": "है उत्तर प्रदेश।",
    "start": 752.44,
    "duration": 4.56
  },
  {
    "text": "भविष्य",
    "start": 758.16,
    "duration": 5.0
  },
  {
    "text": "में कानपुर और",
    "start": 759.88,
    "duration": 6.6
  },
  {
    "text": "यूपी भारत को",
    "start": 763.16,
    "duration": 7.72
  },
  {
    "text": "डिफेंस का बड़ा एक्सपोर्टर बनाने में सबसे",
    "start": 766.48,
    "duration": 7.2
  },
  {
    "text": "आगे रहेंगे।",
    "start": 770.88,
    "duration": 6.399
  },
  {
    "text": "यहां नई फैक्ट्रियां लगेंगी।",
    "start": 773.68,
    "duration": 7.839
  },
  {
    "text": "यहां बड़े पैमाने पर निवेश आएगा।",
    "start": 777.279,
    "duration": 8.481
  },
  {
    "text": "यहां के हजारों युवाओं को रोजगार के अच्छे",
    "start": 781.519,
    "duration": 5.88
  },
  {
    "text": "अवसर",
    "start": 785.76,
    "duration": 4.56
  },
  {
    "text": "मिलेंगे। साथियों,",
    "start": 787.399,
    "duration": 6.041
  },
  {
    "text": "यूपी और कानपुर को विकास की नई ऊंचाई पर",
    "start": 790.32,
    "duration": 4.28
  },
  {
    "text": "लेकर",
    "start": 793.44,
    "duration": 4.48
  },
  {
    "text": "जाना ये डबल इंजन सरकार की पहली",
    "start": 794.6,
    "duration": 5.12
  },
  {
    "text": "प्राथमिकता",
    "start": 797.92,
    "duration": 4.359
  },
  {
    "text": "है। यह तभी",
    "start": 799.72,
    "duration": 7.48
  },
  {
    "text": "होगा जब यहां पर उद्योगों को बढ़ावा",
    "start": 802.279,
    "duration": 6.761
  },
  {
    "text": "मिलेगा।",
    "start": 807.2,
    "duration": 7.92
  },
  {
    "text": "जब कानपुर का पुराना गौरव फिर से लौटेगा।",
    "start": 809.04,
    "duration": 7.56
  },
  {
    "text": "लेकिन भाइयों",
    "start": 815.12,
    "duration": 4.44
  },
  {
    "text": "बहनों पिछली सरकारों",
    "start": 816.6,
    "duration": 7.039
  },
  {
    "text": "ने आधुनिक उद्योगों की इन जरूरतों",
    "start": 819.56,
    "duration": 8.079
  },
  {
    "text": "को नजरअंदाज करके रखा",
    "start": 823.639,
    "duration": 6.721
  },
  {
    "text": "था। कानपुर",
    "start": 827.639,
    "duration": 8.961
  },
  {
    "text": "से उद्योगों का पलायन होता गया।",
    "start": 830.36,
    "duration": 9.039
  },
  {
    "text": "परिवारवादी",
    "start": 836.6,
    "duration": 8.2
  },
  {
    "text": "सरकारें आंख बंद करके बैठी",
    "start": 839.399,
    "duration": 5.401
  },
  {
    "text": "रही। नतीजा यह",
    "start": 844.839,
    "duration": 3.041
  },
  {
    "text": "[संगीत]",
    "start": 846.8,
    "duration": 5.0
  },
  {
    "text": "हुआ कि केवल कानपुर ही",
    "start": 847.88,
    "duration": 6.56
  },
  {
    "text": "नहीं पूरा",
    "start": 851.8,
    "duration": 7.52
  },
  {
    "text": "यूपी पीछे हो गया।",
    "start": 854.44,
    "duration": 4.88
  },
  {
    "text": "भाइयों",
    "start": 859.6,
    "duration": 6.84
  },
  {
    "text": "बहनों राज्य की औद्योगिक प्रगति के",
    "start": 861.24,
    "duration": 9.76
  },
  {
    "text": "लिए दो सबसे जरूरी शर्तें",
    "start": 866.44,
    "duration": 7.72
  },
  {
    "text": "हैं। पहली",
    "start": 871.0,
    "duration": 6.079
  },
  {
    "text": "ऊर्जा क्षेत्र में",
    "start": 874.16,
    "duration": 6.28
  },
  {
    "text": "आत्मनिर्भरता यानी बिजली",
    "start": 877.079,
    "duration": 5.601
  },
  {
    "text": "सप्लाई और",
    "start": 880.44,
    "duration": 4.399
  },
  {
    "text": "दूसरी",
    "start": 882.68,
    "duration": 5.36
  },
  {
    "text": "इंफ्रास्ट्रक्चर और",
    "start": 884.839,
    "duration": 5.761
  },
  {
    "text": "कनेक्टिविटी आज",
    "start": 888.04,
    "duration": 6.159
  },
  {
    "text": "यहां 660 मेगावाट",
    "start": 890.6,
    "duration": 6.799
  },
  {
    "text": "के पनकी पावर",
    "start": 894.199,
    "duration": 9.44
  },
  {
    "text": "प्लांट 660 मेगावाट के नेवेली पावर",
    "start": 897.399,
    "duration": 12.721
  },
  {
    "text": "प्लांट 1320 मेगावाट के जवाहरपुर पावर",
    "start": 903.639,
    "duration": 12.401
  },
  {
    "text": "प्लांट 660 मेगावाट के ओबरासी पावर",
    "start": 910.12,
    "duration": 11.92
  },
  {
    "text": "प्लांट 660 मेगावाट के खुरजा पावर प्लांट",
    "start": 916.04,
    "duration": 9.12
  },
  {
    "text": "का लोकार्पण हुआ",
    "start": 922.04,
    "duration": 6.919
  },
  {
    "text": "है। यह यूपी की",
    "start": 925.16,
    "duration": 7.119
  },
  {
    "text": "ऊर्जा जरूरतों को पूरा करने के",
    "start": 928.959,
    "duration": 6.841
  },
  {
    "text": "लिए बहुत बड़ा कदम",
    "start": 932.279,
    "duration": 7.881
  },
  {
    "text": "है। इन पावर प्लांट के बाद",
    "start": 935.8,
    "duration": 5.44
  },
  {
    "text": "यूपी",
    "start": 940.16,
    "duration": 4.85
  },
  {
    "text": "में बिजली की उपलब्धता और",
    "start": 941.24,
    "duration": 4.959
  },
  {
    "text": "[प्रशंसा]",
    "start": 945.01,
    "duration": 4.269
  },
  {
    "text": "बढ़ेगी। इससे",
    "start": 946.199,
    "duration": 6.241
  },
  {
    "text": "यहां के उद्योगों को भी गति",
    "start": 949.279,
    "duration": 7.161
  },
  {
    "text": "मिलेगी। आज",
    "start": 952.44,
    "duration": 4.0
  },
  {
    "text": "47000 करोड़ से अधिक की लागत",
    "start": 956.6,
    "duration": 7.919
  },
  {
    "text": "के कई और विकास कार्यों का",
    "start": 960.519,
    "duration": 9.481
  },
  {
    "text": "लोकार्पण और शिलान्यास भी किया गया है।",
    "start": 964.519,
    "duration": 7.68
  },
  {
    "text": "यहां बुजुर्गों",
    "start": 970.0,
    "duration": 5.48
  },
  {
    "text": "को मुफ्त इलाज के",
    "start": 972.199,
    "duration": 8.32
  },
  {
    "text": "लिए आयुष्मान वय वंदना कार्ड भी दिए गए",
    "start": 975.48,
    "duration": 8.52
  },
  {
    "text": "हैं। अन्य योजनाओं के लाभार्थियों को भी",
    "start": 980.519,
    "duration": 5.041
  },
  {
    "text": "मदद दी गई",
    "start": 984.0,
    "duration": 4.279
  },
  {
    "text": "है। यह",
    "start": 985.56,
    "duration": 5.76
  },
  {
    "text": "योजनाएं यह विकास",
    "start": 988.279,
    "duration": 7.521
  },
  {
    "text": "कार्य कानपुर और यूपी की प्रगति के",
    "start": 991.32,
    "duration": 9.12
  },
  {
    "text": "लिए हमारे कमिटमेंट को दिखाते",
    "start": 995.8,
    "duration": 6.8
  },
  {
    "text": "हैं।",
    "start": 1000.44,
    "duration": 6.959
  },
  {
    "text": "साथियों आज केंद्र और प्रदेश की",
    "start": 1002.6,
    "duration": 6.64
  },
  {
    "text": "सरकार",
    "start": 1007.399,
    "duration": 5.601
  },
  {
    "text": "आधुनिक और विकसित यूपी के निर्माण के",
    "start": 1009.24,
    "duration": 9.2
  },
  {
    "text": "लिए काम कर रहे हैं। इसी का परिणाम",
    "start": 1013.0,
    "duration": 8.48
  },
  {
    "text": "है जो",
    "start": 1018.44,
    "duration": 5.92
  },
  {
    "text": "इंफ्रास्ट्रक्चर जो",
    "start": 1021.48,
    "duration": 5.839
  },
  {
    "text": "सुविधाएं जो",
    "start": 1024.36,
    "duration": 7.04
  },
  {
    "text": "संसाधन बड़ी-बड़ी मेट्रो सिटीज में होती",
    "start": 1027.319,
    "duration": 6.0
  },
  {
    "text": "है, वह",
    "start": 1031.4,
    "duration": 8.12
  },
  {
    "text": "सब अब अपने कानपुर में भी दिखने लगी",
    "start": 1033.319,
    "duration": 6.201
  },
  {
    "text": "है। कुछ साल",
    "start": 1039.64,
    "duration": 7.0
  },
  {
    "text": "पहले हमारी सरकार ने",
    "start": 1042.679,
    "duration": 8.801
  },
  {
    "text": "कानपुर को पहली मेट्रो की सौगात दी",
    "start": 1046.64,
    "duration": 9.64
  },
  {
    "text": "थी। अब आज कानपुर मेट्रो की ऑरेंज",
    "start": 1051.48,
    "duration": 9.04
  },
  {
    "text": "लाइन कानपुर सेंट्रल तक पहुंच गई",
    "start": 1056.28,
    "duration": 6.639
  },
  {
    "text": "है। पहले",
    "start": 1060.52,
    "duration": 4.96
  },
  {
    "text": "एलिवेटेड और अब",
    "start": 1062.919,
    "duration": 5.76
  },
  {
    "text": "अंडरग्राउंड इस तरह का मेट्रो",
    "start": 1065.48,
    "duration": 6.76
  },
  {
    "text": "नेटवर्क कानपुर के महत्वपूर्ण इलाकों को",
    "start": 1068.679,
    "duration": 5.281
  },
  {
    "text": "जोड़ रहा",
    "start": 1072.24,
    "duration": 5.64
  },
  {
    "text": "है। कानपुर मेट्रो का ये",
    "start": 1073.96,
    "duration": 8.24
  },
  {
    "text": "विस्तार ये कोई साधारण प्रोजेक्ट नहीं",
    "start": 1077.88,
    "duration": 9.2
  },
  {
    "text": "है। कानपुर मेट्रो इस बात का सबूत",
    "start": 1082.2,
    "duration": 8.64
  },
  {
    "text": "है अगर सही",
    "start": 1087.08,
    "duration": 7.2
  },
  {
    "text": "इरादों मजबूत",
    "start": 1090.84,
    "duration": 7.92
  },
  {
    "text": "इच्छाशक्ति और नेक नियत वाली सरकार",
    "start": 1094.28,
    "duration": 8.08
  },
  {
    "text": "हो तो देश के विकास के",
    "start": 1098.76,
    "duration": 6.72
  },
  {
    "text": "लिए प्रदेश के विकास के",
    "start": 1102.36,
    "duration": 7.6
  },
  {
    "text": "लिए कैसे ईमानदारी से प्रयास होते",
    "start": 1105.48,
    "duration": 7.72
  },
  {
    "text": "हैं। आप याद करिए",
    "start": 1109.96,
    "duration": 5.2
  },
  {
    "text": "कानपुर के बारे",
    "start": 1113.2,
    "duration": 6.28
  },
  {
    "text": "में पहले लोग कैसी-कसी बातें करते",
    "start": 1115.16,
    "duration": 6.8
  },
  {
    "text": "थे।",
    "start": 1119.48,
    "duration": 4.96
  },
  {
    "text": "चुन्नीगंज, बड़ा",
    "start": 1121.96,
    "duration": 4.56
  },
  {
    "text": "चौराहा,",
    "start": 1124.44,
    "duration": 4.56
  },
  {
    "text": "नयागंज, कानपुर",
    "start": 1126.52,
    "duration": 6.56
  },
  {
    "text": "सेंट्रल इतनी भीड़भाड़ वाले",
    "start": 1129.0,
    "duration": 9.44
  },
  {
    "text": "इलाके, जगहजगह संकरी सकड़ सड़कों की",
    "start": 1133.08,
    "duration": 9.08
  },
  {
    "text": "समस्या, आधुनिक इंफ्रास्ट्रक्चर",
    "start": 1138.44,
    "duration": 7.239
  },
  {
    "text": "और प्लानिंग की कमी।",
    "start": 1142.16,
    "duration": 8.639
  },
  {
    "text": "लोग कहते थे यहां कहां मेट्रो जैसे काम हो",
    "start": 1145.679,
    "duration": 7.681
  },
  {
    "text": "पाएंगे।",
    "start": 1150.799,
    "duration": 6.12
  },
  {
    "text": "यहां कहां कोई बड़ा बदलाव हो",
    "start": 1153.36,
    "duration": 5.96
  },
  {
    "text": "पाएगा? एक तरह",
    "start": 1156.919,
    "duration": 6.88
  },
  {
    "text": "से कानपुर और यूपी के दूसरे प्रमुख",
    "start": 1159.32,
    "duration": 9.239
  },
  {
    "text": "शहर विकास की दौड़ से बाहर थे।",
    "start": 1163.799,
    "duration": 8.0
  },
  {
    "text": "इससे ट्रैफिक की समस्या गहराती",
    "start": 1168.559,
    "duration": 8.24
  },
  {
    "text": "रही। शहर की रफ्तार कम होती चली गई।",
    "start": 1171.799,
    "duration": 8.401
  },
  {
    "text": "यूपी में सबसे ज्यादा संभावनाओं से भरे",
    "start": 1176.799,
    "duration": 8.161
  },
  {
    "text": "शहर विकास की दौड़ में पिछड़ते गए।",
    "start": 1180.2,
    "duration": 6.64
  },
  {
    "text": "लेकिन आज वही",
    "start": 1184.96,
    "duration": 4.12
  },
  {
    "text": "कानपुर, वही",
    "start": 1186.84,
    "duration": 7.28
  },
  {
    "text": "यूपी विकास के नए कीर्तिमान गढ़ रहा",
    "start": 1189.08,
    "duration": 7.28
  },
  {
    "text": "है। आप",
    "start": 1194.12,
    "duration": 6.36
  },
  {
    "text": "देखिए मेट्रो सेवा सेवाओं से ही कानपुर के",
    "start": 1196.36,
    "duration": 9.52
  },
  {
    "text": "लोगों को कितना फायदा होने जा रहा है।",
    "start": 1200.48,
    "duration": 9.16
  },
  {
    "text": "कानपुर व्यापार का इतना बड़ा केंद्र",
    "start": 1205.88,
    "duration": 7.32
  },
  {
    "text": "है। मेट्रो के कारण अब हमारे व्यापारियों",
    "start": 1209.64,
    "duration": 4.48
  },
  {
    "text": "और",
    "start": 1213.2,
    "duration": 5.28
  },
  {
    "text": "ग्राहकों दोनों के लिए नवीन मार्केट और",
    "start": 1214.12,
    "duration": 8.24
  },
  {
    "text": "बड़ा चौराहारा पहुंचना आसान हो गया",
    "start": 1218.48,
    "duration": 8.88
  },
  {
    "text": "है। कानपुर आने जाने वाले लोग आईआईटी के",
    "start": 1222.36,
    "duration": 9.799
  },
  {
    "text": "स्टूडेंट सामान्य मानवी इन सबके लिए",
    "start": 1227.36,
    "duration": 7.199
  },
  {
    "text": "सेंट्रल रेलवे स्टेशन तक पहुंचने में",
    "start": 1232.159,
    "duration": 4.64
  },
  {
    "text": "कितना समय बचेगा?",
    "start": 1234.559,
    "duration": 3.641
  },
  {
    "text": "हम जानते",
    "start": 1236.799,
    "duration": 4.601
  },
  {
    "text": "हैं शहर की गति",
    "start": 1238.2,
    "duration": 7.04
  },
  {
    "text": "ही शहर की प्रगति बनती",
    "start": 1241.4,
    "duration": 6.159
  },
  {
    "text": "है। यह",
    "start": 1245.24,
    "duration": 6.04
  },
  {
    "text": "सुविधाएं, यह कनेक्टिविटी,",
    "start": 1247.559,
    "duration": 6.081
  },
  {
    "text": "ट्रांसपोर्ट की आधुनिक",
    "start": 1251.28,
    "duration": 7.92
  },
  {
    "text": "फैसिलिटी आज यूपी के आधुनिक विकास की नई",
    "start": 1253.64,
    "duration": 7.44
  },
  {
    "text": "तस्वीर बन रही",
    "start": 1259.2,
    "duration": 3.719
  },
  {
    "text": "है।",
    "start": 1261.08,
    "duration": 5.479
  },
  {
    "text": "साथियों आज हमारा यूपी",
    "start": 1262.919,
    "duration": 5.601
  },
  {
    "text": "आधुनिक",
    "start": 1266.559,
    "duration": 5.24
  },
  {
    "text": "इंफ्रास्ट्रक्चर और कनेक्टिविटी के मामले",
    "start": 1268.52,
    "duration": 6.8
  },
  {
    "text": "में बहुत आगे निकल रहा",
    "start": 1271.799,
    "duration": 7.561
  },
  {
    "text": "है। जिस यूपी की पहचान",
    "start": 1275.32,
    "duration": 8.4
  },
  {
    "text": "टूटी फूटी सड़कों और गड्ढों से होती थी वो",
    "start": 1279.36,
    "duration": 8.08
  },
  {
    "text": "अब एक्सप्रेसवे के नेटवर्क के लिए जाना",
    "start": 1283.72,
    "duration": 4.56
  },
  {
    "text": "जाता",
    "start": 1287.44,
    "duration": 5.76
  },
  {
    "text": "है। जिस यूपी में लोग शाम के बाद बाहर",
    "start": 1288.28,
    "duration": 10.44
  },
  {
    "text": "जाने से बचते थे। वहां अब हाईवेज पर 24ों",
    "start": 1293.2,
    "duration": 10.599
  },
  {
    "text": "घंटे लोग ट्रैवल करते हैं। यूपी कैसे बदला",
    "start": 1298.72,
    "duration": 9.12
  },
  {
    "text": "है? यह कानपुर वालों से बेहतर भला कौन",
    "start": 1303.799,
    "duration": 5.841
  },
  {
    "text": "जानता",
    "start": 1307.84,
    "duration": 6.64
  },
  {
    "text": "है? कुछ ही दिनों में कानपुर लखनऊ",
    "start": 1309.64,
    "duration": 9.12
  },
  {
    "text": "एक्सप्रेसवे से लखनऊ का सफर",
    "start": 1314.48,
    "duration": 10.64
  },
  {
    "text": "सिर्फ 40 45 मिनट का होने वाला है।",
    "start": 1318.76,
    "duration": 9.48
  },
  {
    "text": "यह बेटी कब से खड़ी है? चित्र लेकर के थक",
    "start": 1325.12,
    "duration": 7.52
  },
  {
    "text": "गई होगी। जरा एसपीजी के लोग इस बेटी से वो",
    "start": 1328.24,
    "duration": 6.64
  },
  {
    "text": "चित्र ले लीजिए।",
    "start": 1332.64,
    "duration": 5.519
  },
  {
    "text": "थैंक यू बेटा। बहुत बढ़िया शानदार चित्र",
    "start": 1334.88,
    "duration": 4.19
  },
  {
    "text": "बना के लाई हो",
    "start": 1338.159,
    "duration": 3.4
  },
  {
    "text": "[संगीत]",
    "start": 1339.07,
    "duration": 5.969
  },
  {
    "text": "आप। देखिए इस बच्ची के कब से थक जाएगी",
    "start": 1341.559,
    "duration": 7.0
  },
  {
    "text": "बेटा। वो तुम्हारा नाम पता लिख दिया है",
    "start": 1345.039,
    "duration": 6.481
  },
  {
    "text": "बेटा।",
    "start": 1348.559,
    "duration": 5.721
  },
  {
    "text": "वो वो मेरी ऑफिस के लोग आएंगे अभी ले",
    "start": 1351.52,
    "duration": 5.48
  },
  {
    "text": "लेंगे। मुझे पहुंच जाएगा",
    "start": 1354.28,
    "duration": 7.84
  },
  {
    "text": "बेटा। बहुत-बहुत धन्यवाद आपका।",
    "start": 1357.0,
    "duration": 5.12
  },
  {
    "text": "[संगीत]",
    "start": 1365.05,
    "duration": 6.03
  },
  {
    "text": "साथियों, लखनऊ से",
    "start": 1367.88,
    "duration": 7.679
  },
  {
    "text": "ही पूर्वांचल एक्सप्रेसवे से",
    "start": 1371.08,
    "duration": 8.839
  },
  {
    "text": "सीधे कनेक्टिविटी भी मिलेगी।",
    "start": 1375.559,
    "duration": 7.521
  },
  {
    "text": "कानपुर लखनऊ एक्सप्रेसवे",
    "start": 1379.919,
    "duration": 7.561
  },
  {
    "text": "को गंगा एक्सप्रेसवे से भी जोड़ा",
    "start": 1383.08,
    "duration": 7.599
  },
  {
    "text": "जाएगा। इससे पूर्व और",
    "start": 1387.48,
    "duration": 9.24
  },
  {
    "text": "पश्चिम दोनों तरफ जाने के लिए दूरी भी और",
    "start": 1390.679,
    "duration": 9.681
  },
  {
    "text": "समय भी बचेगा।",
    "start": 1396.72,
    "duration": 7.0
  },
  {
    "text": "साथियों कानपुर के लोगों को अब",
    "start": 1400.36,
    "duration": 5.52
  },
  {
    "text": "तक",
    "start": 1403.72,
    "duration": 5.28
  },
  {
    "text": "फर्रूखाबाद अनवरगंज सेक्शन",
    "start": 1405.88,
    "duration": 7.6
  },
  {
    "text": "में सिंगल लाइन से दिक्कत होती रही",
    "start": 1409.0,
    "duration": 10.159
  },
  {
    "text": "है। एक दो नहीं 18 रेलवे क्रॉसिंग",
    "start": 1413.48,
    "duration": 10.76
  },
  {
    "text": "से आपको संघर्ष करना पड़ता था।",
    "start": 1419.159,
    "duration": 7.201
  },
  {
    "text": "कभी यह फाटक",
    "start": 1424.24,
    "duration": 5.08
  },
  {
    "text": "बंद, कभी वह फाटक",
    "start": 1426.36,
    "duration": 6.679
  },
  {
    "text": "बंद। आप लोग कब से इस परेशानी से मुक्ति",
    "start": 1429.32,
    "duration": 7.44
  },
  {
    "text": "की मांग कर रहे थे। अब यहां",
    "start": 1433.039,
    "duration": 10.081
  },
  {
    "text": "भी ₹1000 करोड़ खर्च करके एलिवेटेड रेल",
    "start": 1436.76,
    "duration": 10.159
  },
  {
    "text": "कॉरिडोर बनने जा रहा",
    "start": 1443.12,
    "duration": 6.12
  },
  {
    "text": "है। इससे",
    "start": 1446.919,
    "duration": 6.041
  },
  {
    "text": "यहां ट्रैफिक सुधरेगा।",
    "start": 1449.24,
    "duration": 5.12
  },
  {
    "text": "स्पीड",
    "start": 1452.96,
    "duration": 4.76
  },
  {
    "text": "बढ़ेगी और प्रदूषण भी कम",
    "start": 1454.36,
    "duration": 6.319
  },
  {
    "text": "होगा और सबसे बड़ी",
    "start": 1457.72,
    "duration": 10.16
  },
  {
    "text": "बात कानपुर के आप लोगों का समय बचेगा।",
    "start": 1460.679,
    "duration": 7.201
  },
  {
    "text": "साथियों कानपुर सेंट्रल रेलवे स्टेशन को",
    "start": 1468.039,
    "duration": 6.481
  },
  {
    "text": "भी अपग्रेड",
    "start": 1471.88,
    "duration": 7.399
  },
  {
    "text": "करके विश्वस्तरीय लुक दिया जा रहा है।",
    "start": 1474.52,
    "duration": 9.0
  },
  {
    "text": "थोड़े ही समय में कानपुर सेंट्रल रेलवे",
    "start": 1479.279,
    "duration": 7.561
  },
  {
    "text": "स्टेशन भी एयरपोर्ट की",
    "start": 1483.52,
    "duration": 5.08
  },
  {
    "text": "तरह",
    "start": 1486.84,
    "duration": 6.56
  },
  {
    "text": "आधुनिक वर्ल्ड क्लास आपको नजर",
    "start": 1488.6,
    "duration": 8.6
  },
  {
    "text": "आएगा। हमारी सरकार",
    "start": 1493.4,
    "duration": 5.12
  },
  {
    "text": "यूपी",
    "start": 1497.2,
    "duration": 7.12
  },
  {
    "text": "के 150 से ज्यादा रेलवे स्टेशनों को अमृत",
    "start": 1498.52,
    "duration": 8.36
  },
  {
    "text": "भारत रेलवे स्टेशन के रूप में विकसित कर",
    "start": 1504.32,
    "duration": 5.16
  },
  {
    "text": "रही है।",
    "start": 1506.88,
    "duration": 5.799
  },
  {
    "text": "यूपी पहले ही देश में सबसे",
    "start": 1509.48,
    "duration": 6.84
  },
  {
    "text": "ज्यादा इंटरनेशनल एयरपोर्ट वाला राज्य बन",
    "start": 1512.679,
    "duration": 8.88
  },
  {
    "text": "चुका है। यानी हाईवेज,",
    "start": 1516.32,
    "duration": 8.4
  },
  {
    "text": "रेलवेज और एयरवेज।",
    "start": 1521.559,
    "duration": 7.321
  },
  {
    "text": "यूपी अब हर क्षेत्र में तेजी से आगे बढ़",
    "start": 1524.72,
    "duration": 6.28
  },
  {
    "text": "रहा है।",
    "start": 1528.88,
    "duration": 6.96
  },
  {
    "text": "साथियों, हम यूपी को औद्योगिक संभावनाओं",
    "start": 1531.0,
    "duration": 9.279
  },
  {
    "text": "का राज्य बना रहे हैं। इस साल के बजट में",
    "start": 1535.84,
    "duration": 8.719
  },
  {
    "text": "हमने मेक इन इंडिया के लिए मिशन",
    "start": 1540.279,
    "duration": 9.0
  },
  {
    "text": "मैन्युफैक्चरिंग की घोषणा की है। इसके तहत",
    "start": 1544.559,
    "duration": 9.401
  },
  {
    "text": "लोकल उद्योगों को उत्पादन को बढ़ावा दिया",
    "start": 1549.279,
    "duration": 8.161
  },
  {
    "text": "जाएगा। कानपुर जैसे शहरों को इसका बहुत",
    "start": 1553.96,
    "duration": 4.959
  },
  {
    "text": "बड़ा लाभ",
    "start": 1557.44,
    "duration": 5.68
  },
  {
    "text": "मिलेगा। आप जानते ही हैं कानपुर के",
    "start": 1558.919,
    "duration": 7.76
  },
  {
    "text": "औद्योगिक सामर्थ्य में सबसे बड़ा",
    "start": 1563.12,
    "duration": 5.799
  },
  {
    "text": "योगदान यहां की",
    "start": 1566.679,
    "duration": 6.6
  },
  {
    "text": "एमएसएमई लघु उद्योगों का होता था। आज हम",
    "start": 1568.919,
    "duration": 7.0
  },
  {
    "text": "यहां के लघु उद्योगों की अपेक्षाओं को",
    "start": 1573.279,
    "duration": 6.321
  },
  {
    "text": "पूरा करते हुए काम कर रहे हैं। साथियों",
    "start": 1575.919,
    "duration": 5.64
  },
  {
    "text": "कुछ समय पहले",
    "start": 1579.6,
    "duration": 7.439
  },
  {
    "text": "तक हमारी एमएसएमस को इस तरह परिभाषित किया",
    "start": 1581.559,
    "duration": 9.081
  },
  {
    "text": "जाता था कि उन्हें विस्तार करने में भी डर",
    "start": 1587.039,
    "duration": 8.201
  },
  {
    "text": "लगता था। हमने उन पुरानी परिभाषाओं को",
    "start": 1590.64,
    "duration": 7.72
  },
  {
    "text": "बदला। हमने लघु उद्योगों के",
    "start": 1595.24,
    "duration": 7.4
  },
  {
    "text": "टर्नओवर और स्केल की सीमा को बढ़ाया।",
    "start": 1598.36,
    "duration": 9.96
  },
  {
    "text": "इस बजट में सरकार ने एक बार फिर एमएसएमईस",
    "start": 1602.64,
    "duration": 9.6
  },
  {
    "text": "के दायरे को और बढ़ाते हुए उन्हें और भी",
    "start": 1608.32,
    "duration": 9.88
  },
  {
    "text": "छूट दी है। पहले के समय में एमएसएमईस के",
    "start": 1612.24,
    "duration": 7.96
  },
  {
    "text": "सामने बड़ी",
    "start": 1618.2,
    "duration": 5.8
  },
  {
    "text": "दिक्कत क्रेडिट की भी होती थी।",
    "start": 1620.2,
    "duration": 7.32
  },
  {
    "text": "पिछले 10 वर्षों में हमने क्रेडिट की",
    "start": 1624.0,
    "duration": 7.36
  },
  {
    "text": "समस्या को खत्म करने के लिए एक के बाद एक",
    "start": 1627.52,
    "duration": 6.2
  },
  {
    "text": "कई बड़े फैसले",
    "start": 1631.36,
    "duration": 4.36
  },
  {
    "text": "लिए। आज",
    "start": 1633.72,
    "duration": 6.52
  },
  {
    "text": "युवा अपना उद्योग शुरू करना चाहते हैं तो",
    "start": 1635.72,
    "duration": 7.079
  },
  {
    "text": "उन्हें मुद्रा योजना के जरिए तुरंत पूंजी",
    "start": 1640.24,
    "duration": 6.08
  },
  {
    "text": "मिल जाती है। छोटे और मध्यम उद्योगों को",
    "start": 1642.799,
    "duration": 6.561
  },
  {
    "text": "आर्थिक मजबूती देने के लिए हमने क्रेडिट",
    "start": 1646.32,
    "duration": 5.44
  },
  {
    "text": "गारंटी स्कीम चलाई।",
    "start": 1649.36,
    "duration": 6.559
  },
  {
    "text": "इस साल के बजट में एमएसएम लोन पर गारंटी",
    "start": 1651.76,
    "duration": 9.2
  },
  {
    "text": "को बढ़ाकर 20 करोड़ कर दिया गया। एमएसएमएस",
    "start": 1655.919,
    "duration": 9.041
  },
  {
    "text": "के लिए 5 लाख तक की लिमिट वाले क्रेडिट",
    "start": 1660.96,
    "duration": 9.4
  },
  {
    "text": "कार्ड भी दिए जा रहे हैं। हम यहां पर नए",
    "start": 1664.96,
    "duration": 9.68
  },
  {
    "text": "उद्योगों खासकर एमएसएमएस के लिए अनुकूल",
    "start": 1670.36,
    "duration": 8.36
  },
  {
    "text": "माहौल बना रहे हैं। इसके लिए प्रक्रियाओं",
    "start": 1674.64,
    "duration": 8.32
  },
  {
    "text": "को सरल किया जा रहा है। कानपुर के",
    "start": 1678.72,
    "duration": 8.4
  },
  {
    "text": "पारंपरिक चमड़ा और होजरी उद्योग को वन",
    "start": 1682.96,
    "duration": 6.24
  },
  {
    "text": "डिस्ट्रिक्ट वन प्रोडक्ट जैसी योजनाओं के",
    "start": 1687.12,
    "duration": 6.799
  },
  {
    "text": "माध्यम से सशक्त किया जा रहा है। हमारे इन",
    "start": 1689.2,
    "duration": 9.28
  },
  {
    "text": "प्रयासों का लाभ कानपुर के साथ-साथ",
    "start": 1693.919,
    "duration": 8.321
  },
  {
    "text": "यूपी के सभी जिलों को भी मिलेगा। साथियों",
    "start": 1698.48,
    "duration": 6.96
  },
  {
    "text": "आज उत्तर प्रदेश में निवेश का एक",
    "start": 1702.24,
    "duration": 7.2
  },
  {
    "text": "अभूतपूर्व सुरक्षित माहौल बना है। गरीब",
    "start": 1705.44,
    "duration": 7.839
  },
  {
    "text": "कल्याण की योजनाओं को पारदर्शिता से जमीन",
    "start": 1709.44,
    "duration": 7.52
  },
  {
    "text": "पर उतारा जा रहा है। मध्यम वर्ग के सपनों",
    "start": 1713.279,
    "duration": 7.041
  },
  {
    "text": "को पूरा करने के लिए भी सरकार उनके साथ",
    "start": 1716.96,
    "duration": 8.88
  },
  {
    "text": "खड़ी है। इस बजट में हमने ₹1 लाख तक की आय",
    "start": 1720.32,
    "duration": 9.2
  },
  {
    "text": "को पूरी तरह टैक्स फ्री कर दिया है। इससे",
    "start": 1725.84,
    "duration": 7.04
  },
  {
    "text": "करोड़ों मध्यम वर्गीय परिवारों में नया",
    "start": 1729.52,
    "duration": 8.24
  },
  {
    "text": "विश्वास जगा है। उन्हें नई ताकत मिली है।",
    "start": 1732.88,
    "duration": 8.96
  },
  {
    "text": "हम सेवा और विकास के संकल्प के साथ इसी",
    "start": 1737.76,
    "duration": 9.48
  },
  {
    "text": "तरह तेजी से आगे बढ़ेंगे। हम देश को यूपी",
    "start": 1741.84,
    "duration": 9.4
  },
  {
    "text": "को नई ऊंचाई पर लेकर जाने के",
    "start": 1747.24,
    "duration": 7.4
  },
  {
    "text": "लिए मेहनत करने में कोई कोर कसर नहीं",
    "start": 1751.24,
    "duration": 5.48
  },
  {
    "text": "छोड़ेंगे।",
    "start": 1754.64,
    "duration": 5.44
  },
  {
    "text": "मैं कानपुर के उज्जवल भविष्य के लिए सभी",
    "start": 1756.72,
    "duration": 6.48
  },
  {
    "text": "मेरे कानपुर के भाई बहनों को बहुत-बहुत",
    "start": 1760.08,
    "duration": 8.44
  },
  {
    "text": "शुभकामनाएं देता हूं। बहुत-बहुत धन्यवाद।",
    "start": 1763.2,
    "duration": 5.32
  },
  {
    "text": "[संगीत]",
    "start": 1773.52,
    "duration": 5.23
  },
  {
    "text": "[प्रशंसा]",
    "start": 1775.6,
    "duration": 3.15
  },
  {
    "text": "[संगीत]",
    "start": 1784.46,
    "duration": 4.849
  },
  {
    "text": "[प्रशंसा]",
    "start": 1786.05,
    "duration": 3.259
  },
  {
    "text": "एंड देयर यू सी द प्राउड्स चीयरिंग प्राइम",
    "start": 1790.0,
    "duration": 6.159
  },
  {
    "text": "मिनिस्टर नरेंद्र मोदी आफ्टर दैट एड्रेस।",
    "start": 1792.08,
    "duration": 5.92
  },
  {
    "text": "लाउड मोदी चांस एंड प्राइम मिनिस्टर",
    "start": 1796.159,
    "duration": 4.161
  },
  {
    "text": "नरेंद्र मोदी टॉकिंग ऑन दैट बिग इंफ्रा",
    "start": 1798.0,
    "duration": 4.88
  },
  {
    "text": "पुश वेयर ही सेड हाउ लाइफ विल बी सो मच",
    "start": 1800.32,
    "duration": 5.2
  },
  {
    "text": "मोर कन्वीनिएंट फॉर द पीपल विथ इंप्रूव्ड",
    "start": 1802.88,
    "duration": 5.039
  },
  {
    "text": "अर्बन मोबिलिटी कनेक्टिंग की सिटी",
    "start": 1805.52,
    "duration": 4.24
  },
  {
    "text": "लैंडमार्क्स कमर्शियल हाउस देयर बाय",
    "start": 1807.919,
    "duration": 3.921
  },
  {
    "text": "बूस्टिंग पब्लिक ट्रांसपोर्टेशन एंड ईिंग",
    "start": 1809.76,
    "duration": 5.639
  },
  {
    "text": "सिटी ट्रैफिक",
    "start": 1811.84,
    "duration": 3.559
  }
]